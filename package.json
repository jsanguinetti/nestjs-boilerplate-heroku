{
   "name": "nest-typescript-starter",
   "version": "1.0.0",
   "description": "Nest TypeScript starter repository",
   "license": "MIT",
   "scripts": {
      "start": "node index.js",
      "start:watch": "nodemon",
      "foreman": "foreman start -f Procfile.dev",
      "prestart:prod": "yarn tsc",
      "start:prod": "node dist/main.js",
      "migrations:generate": "yarn ts-node ./node_modules/typeorm/cli.js migrations:generate",
      "migration:run": "yarn ts-node ./node_modules/typeorm/cli.js migration:run",
      "migration:revert": "yarn ts-node ./node_modules/typeorm/cli.js migration:revert",
      "migration:run:prod": "yarn tsc && yarn ts-node ./node_modules/typeorm/cli.js migration:run",
      "lint": "yarn tslint -t codeFrame 'src/**/*.ts'"
   },
   "dependencies": {
      "@nestjs/common": "^6.0.0",
      "@nestjs/core": "^6.0.0",
      "@nestjs/microservices": "^6.0.0",
      "@nestjs/platform-express": "^6.2.4",
      "@nestjs/swagger": "^3.0.0",
      "@nestjs/testing": "^6.0.0",
      "@nestjs/typeorm": "^6.0.0",
      "@nestjs/websockets": "^6.0.0",
      "bcrypt": "^3.0.0",
      "class-validator": "^0.8.1",
      "cors": "^2.8.4",
      "dotenv": "^8.0.0",
      "jsonwebtoken": "^8.5.0",
      "morgan": "^1.9.0",
      "passport": "^0.4.0",
      "passport-jwt": "^4.0.0",
      "pg": "^7.11.0",
      "redis": "^2.8.0",
      "reflect-metadata": "^0.1.10",
      "request": "^2.88.0",
      "rxjs": "^6.5.0",
      "swagger-ui-express": "^4.0.5",
      "ts-node": "~8.2.0",
      "typeorm": "0.2.17",
      "typescript": "~3.5.0",
      "@types/node": "^10.0.0",
      "@types/express": "4.16.1"
   },
   "devDependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/jsonwebtoken": "^8.3.2",
      "@types/passport-jwt": "^3.0.1",
      "nodemon": "^1.19.0",
      "tslint": "^5.17.0"
   },
   "engines": {
      "node": "10"
   }
}
